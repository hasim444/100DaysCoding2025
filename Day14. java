public class projek_belajar {
    public static void main(String[] args) {
        // Data bentuk String
        String sAngka   = "123";
        String sInteger = "18";
        String sDouble  = "63.5";
        String sFloat   = "170.5";
        String sLong    = "1000000";
        String sShort   = "100";
        String sByte    = "18";
        String sBoolean = "true";
        String sChar    = "Z";
        
        int angka       = Integer.parseInt(sAngka);
        int integer     = Integer.parseInt(sInteger);
        double pecahan  = Double.parseDouble(sDouble);
        float bilangan  = Float.parseFloat(sFloat);
        long panjang    = Long.parseLong(sLong);
        short kecil     = Short.parseShort(sShort);
        byte umur       = Byte.parseByte(sByte);
        boolean status  = Boolean.parseBoolean(sBoolean);
        char huruf      = sChar.charAt(0);
        
        System.out.println("Integer \t: " + angka);
        System.out.println("int \t\t: " + integer);
        System.out.println("Double \t\t: " + pecahan);
        System.out.println("Float \t\t: " + bilangan);
        System.out.println("Long \t\t: " + panjang);
        System.out.println("Short \t\t: " + kecil);
        System.out.println("Byte \t\t: " + umur);
        System.out.println("Boolean \t: " + status);
        System.out.println("Char \t\t: " + huruf);
    }
}
